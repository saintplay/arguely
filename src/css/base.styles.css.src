/* eslint-disable */

@tailwind base;

*::-webkit-scrollbar-track
{
	-webkit-box-shadow: inset 0 0 4px rgba(0,0,0,0.3);
	border-radius: 6px;
}
*::-webkit-scrollbar
{
	width: 8px;
}
*::-webkit-scrollbar-thumb
{
	border-radius: 8px;
	-webkit-box-shadow: inset 0 0 5px rgba(0,0,0,.3);
	background-color: #555;
}
* {
  scrollbar-color: #555 rgba(0,0,0,0.3);
}

@tailwind components;
@tailwind utilities;
