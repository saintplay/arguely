/* eslint-disable */

@tailwind base;

*::-webkit-scrollbar-track
{
	-webkit-box-shadow: inset 0 0 0 rgba(0,0,0,0);
	border-radius: 5px;
}
*::-webkit-scrollbar
{
	width: 8px;
}
*::-webkit-scrollbar-thumb
{
	border-radius: 8px;
	-webkit-box-shadow: inset 0 0 0 rgba(0,0,0,0);
	background-color: #333;
}
* {
  scrollbar-color: #333 transparent;
}

input:focus {
   outline:0;
}

@tailwind components;
@tailwind utilities;
